.QTimeEdit::up-arrow{
     image:url(:/up.svg);
}

.QTimeEdit::down-arrow {
    image:url(:/down.svg);
}

.QTimeEdit::down-button
{
    border:none;
    background-color:white;
}

.QTimeEdit::up-button
{
    border:none;
    background-color:white;
}

.QTimeEdit::down-arrow:on{
   color:#409eff;
}

.QCheckBox {
    color: #606266;
    font-weight: 100;
    font-size: 14px;
    position: relative;
    cursor: pointer;
    display: inline-block;
    white-space: nowrap;
    user-select: none;
    margin-right: 30px
}

.QCheckBox:hover
{
   color: #409eff;
}

.QCheckBox:checked{
   color: #409eff;
}

.QCheckBox::indicator:hover {
    color: #409eff;
}

.QCheckBox::indicator:checked
{
    display: inline-block;
    position: relative;
    border: 1px solid #409eff;
    border-radius: 2px;
    box-sizing: border-box;
    width: 14px;
    height: 14px;
    background-color: #409eff;
    color: #409eff;
    z-index: 1;
    border-image: url(:/tick.svg);
}

.QCheckBox::indicator:unchecked {
    display: inline-block;
    position: relative;
    border: 1px solid #dcdfe6;
    border-radius: 2px;
    box-sizing: border-box;
    width: 14px;
    height: 14px;
    background-color: #fff;
    z-index: 1;
}

.QCheckBox::indicator:unchecked:hover{
    border: 1px solid #409eff;
}


.QRadioButton {
    color: #606266;
    font-weight: 100;
    line-height: 1;
    position: relative;
    cursor: pointer;
    display: inline-block;
    white-space: nowrap;
    outline: none;
    font-size: 14px;
    margin-right: 30px;
}

.QRadioButton:disabled{
    color: blue;
    border-color: brown;
    background-color: aqua;
}


.QRadioButton:hover{
        color: #409eff;
}
.QRadioButton:pressed{
   color: #409eff;
}
.QRadioButton:checked
{
   color: #409eff;
}

.QRadioButton::indicator {
    width: 32px;
    height: 32px;
    position: relative;
    color: #409eff;
}

.QRadioButton::indicator:hover {
    color: #409eff;
}

.QRadioButton::indicator:pressed {
    color: #409eff;
}

.QRadioButton::indicator:checked
{
    position: relative;
    border: 1px solid #409eff;
    border-radius: 7px;
    box-sizing: border-box;
    width: 14px;
    height: 14px;
    background-color: #409eff;
    color: #409eff;
    border-image: url(:/QCheckBox.svg);
}
.QRadioButton::indicator:unchecked {
    display: inline-block;
    position: relative;
    border: 1px solid #dcdfe6;
    border-radius: 7px;
    box-sizing: border-box;
    width: 14px;
    height: 14px;
    background-color: #fff;
}

.QRadioButton::indicator:unchecked:hover{
    border: 1px solid #409eff;
}

.QTextEdit{
    height:30px;
}


QPushButton,
QToolButton {
    display: inline-block;
    line-height: 1;
    white-space: nowrap;
    cursor: pointer;
    background: #fff;
    border: 1px solid #dcdfe6;
    color: #606266;
    text-align: center;
    box-sizing: border-box;
    outline: none;
    margin: 0;
    transition: .1s;
    font-weight: 100;
    padding: 12px 20px;
    font-size: 14px;
    border-radius: 4px;
    font-family: "FontAwesome";
}

QPushButton:hover,
QToolButton:hover {
    color: #409eff;
    border-color: #c6e2ff;
    background-color: #ecf5ff;
}

QPushButton:pressed,
QToolButton:pressed {
    color: #3a8ee6;
    border-color: #3a8ee6;
    outline: none;
}


/* primary */
QToolButton[type="primary"],
QPushButton[type="primary"] {
    color: #fff;
    background-color: #409eff;
    border-color: #409eff;
}
QToolButton[type="primary"]:hover,
QPushButton[type="primary"]:hover {
    background: #66b1ff;
    border-color: #66b1ff;
    color: #fff;
}
QToolButton[type="primary"]:pressed,
QPushButton[type="primary"]:pressed {
    background: #3a8ee6;
    border-color: #3a8ee6;
    color: #fff;
    outline: none;
}
