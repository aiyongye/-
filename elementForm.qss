#Widget{background: #fff;}

#widget01{
    background: #fff;
    border-radius: 4px;
    border: 1px solid #dcdfe6;
}

.QLabel[type="h3"] {
    font-size: 22px;
    font-weight: 500;
    color: #1f2f3d;
}

.QLabel{
    font-size: 14px;
    color: #5e6d82;
    line-height: 1.5em;
}


.QLineEdit {
    -webkit-appearance: none;
    background-color: #fff;
    background-image: none;
    border-radius: 4px;
    border: 1px solid #dcdfe6;
    box-sizing: border-box;
    color: #606266;
    display: inline-block;
    font-size: inherit;
    height: 40px;
    line-height: 40px;
    outline: none;
    padding: 0 15px;
    transition: border-color .2s cubic-bezier(.645,.045,.355,1);
    width: 100%;
}
.QPushButton {
    -webkit-appearance: none;
    background-color: #fff;
    background-image: none;
    border-radius: 4px;
    border: 1px solid #dcdfe6;
    box-sizing: border-box;
    color: #606266;
    display: inline-block;
    font-size: inherit;
    height: 40px;
    line-height: 40px;
    outline: none;
    padding: 0 15px;
    transition: border-color .2s cubic-bezier(.645,.045,.355,1);
    width: 100%;
}

.QTextEdit{
    background-color: #fff;
    background-image: none;
    border-radius: 4px;
    border: 1px solid #dcdfe6;
    box-sizing: border-box;
    color: #606266;
    display: inline-block;
    font-size: inherit;
    outline: none;
    padding: 0 15px;
    transition: border-color .2s cubic-bezier(.645,.045,.355,1);
    width: 100%;
}

.QLineEdit:hover,.QTextEdit:hover{
    border: 1px solid #409eff;
}

.QLineEdit:pressed,.QTextEdit:pressed{
   border: 1px solid #409eff;
}
.QLineEdit:checked,.QTextEdit:checked{
   border: 1px solid #409eff;
}

.QLabel {
    text-align: right;
    vertical-align: middle;
    float: left;
    font-size: 14px;
    color: #606266;
    line-height: 40px;
    padding: 0 12px 0 0;
    box-sizing: border-box;
}

.QComboBox{
    cursor: pointer;
    background-color: #fff;
    background-image: none;
    border-radius: 4px;
    border: 1px solid #dcdfe6;
    box-sizing: border-box;
    color: #606266;
    display: inline-block;
    font-size: inherit;
    height: 40px;
    line-height: 40px;
    outline: none;
    padding: 0 15px;
    width: 100%;
}

.QComboBox QAbstractItemView{
    background: #fff;
    color: #606266;
    border:1px solid #dcdfe6;
    border-radius:0px 0px 5px 5px;
    outline: 0px;
}

.QComboBox QAbstractItemView::item {
    min-height: 40px;
    line-height: 40px;
}

.QComboBox QAbstractItemView::item:hover{
  background-color:#f5f7fa;
  color: #606266;
}

.QComboBox QAbstractItemView::item:selected{
  background-color:#f5f7fa;
  color: #606266;
}
.QComboBox::drop-down{
    padding: 0 20px 0 0;
    border: none;
}

.QComboBox::down-arrow:on{
   image:url(:/up.svg);
}

.QComboBox::down-arrow {
    image:url(:/down.svg);
}

.QDateEdit,.QTimeEdit{
    background-color: #fff;
    background-image: none;
    border-radius: 4px;
    border: 1px solid #dcdfe6;
    box-sizing: border-box;
    color: #606266;
    display: inline-block;
    font-size: inherit;
    height: 40px;
    line-height: 40px;
    outline: none;
    padding: 0 15px;
    width: 100%;
}
.QDateEdit:hover,.QComboBox:hover,.QTimeEdit:hover{
    cursor: pointer;
    outline: none;
    border-color: #409eff;
}
.QDateEdit:focus,.QComboBox:focus,.QTimeEdit:focus{
    cursor: pointer;
    outline: none;
    border-color: #409eff;
}
